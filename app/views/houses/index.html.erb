<h1>The houses</h1>
<% @houses.each do |house| %>
  <div class="house">
  <h2><%= link_to house.name, house_path(house) %></h2>
  <img src="<%= house.img_url %>">
  <h3>Religion: <%= house.religion %></h3>
    <h3>Region: <%= house.region %></h3>
    <h3>Age: <%= house.age %></h3>
    <h3>Founder: <%= house.founder %></h3>
    <h3>Weapon: <%= house.weapon %></h3>
</div>
<% end %>
